<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{70} [ytelse=%X{ytelse}, sid=%X{soknad_id}, cid=%X{correlation_id}] - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="org.apache.kafka" level="WARN"/>

	<root level="info">
		<appender-ref ref="stdout" />
	</root>
</configuration>
